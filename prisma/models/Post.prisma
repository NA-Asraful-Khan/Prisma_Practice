model Post {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  title     String   @db.VarChar(255)
  content   String?
  imageUrl  String?
  author    User     @relation("UserToPost", fields: [authorId], references: [id], onDelete: Cascade)
  authorId  Int
}